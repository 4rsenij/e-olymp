using System;

namespace Problem494
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            string s = Console.ReadLine();
            int length = s.Length;
            int say = 0;
            for (int i = 0; i < length; i++)
            {
                if (s[i] == 'A' || s[i] == 'E' || s[i] == 'I' || s[i] == 'O' || s[i] == 'U' || s[i] == 'Y') say++;
            }
            Console.WriteLine(say);
        }
    }
}
