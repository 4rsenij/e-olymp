#include <iostream>
#include <fstream>
#include <string>

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

typedef long long ll;

long n, a, b;
string s;

int main()
{
	cin >> n >> a >> b;
	if (a + b == 0) cout << 0 << endl;
	else
	{
		if (a == b | !(a*b)) cout << 1;
		else cout << 2;
		for (int i = 0; i < n - 1; i++) s += '0';
		cout << s << endl;
	}
}
