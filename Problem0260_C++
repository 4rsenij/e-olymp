#include <iostream>
#include <fstream>
#include <vector>
#include <algorithm>


#define REP(i,n) for((i) = 0; (i) < (int)(n); (i)++)

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

typedef long long ll;
typedef vector<ll> vll;

ll n, t, i;
vll v;

bool f(ll a, ll b)
{
	return a > b;
}

int main()
{
	cin >> n;
	REP(i, n)
	{
		cin >> t;
		v.push_back(t);
	}
	sort(v.begin(), v.end(), f);
	if (v.size() > 0)
	{
		cout << v[0];
		for (int i = 1; i < v.size(); i++) cout << " " << v[i];
	}
	cout << endl;
}
