#include <iostream>
#include <fstream>
#include <string>
#include <cstdlib>

#define ln length()
#define FOR(i,n) for((i) = 0; (i) < (int)(n); (i)++)

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

typedef long long ll;

string a, b;
ll C[26], n, tmp, i, j;

int main()
{
	cin >> n;
	getline(cin, a);
	FOR(i, n)
	{
		getline(cin, a);
		getline(cin, b);
		tmp = 0;
		FOR(j, 26) C[j] = 0;
		FOR(j, a.ln) C[a[j] - 97]++;
		FOR(j, b.ln) C[b[j] - 97]--;
		FOR(j, 26) tmp += abs(C[j]);
		cout << "Case #" << i + 1 << ":  " << tmp << endl;
	}
}
