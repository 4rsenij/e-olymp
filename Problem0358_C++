#include <iostream>
#include <fstream>
#include <string>

#define REP(i,n) for((i) = 0; (i) < (int)(n); (i)++)
#define len(x) ((int)(x).length())

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

//typedef long double ld;
typedef long long ll;

string s;
ll ans;

int main()
{
	cin >> s;
	int i;
	REP(i, len(s))
	{
		switch (s[i])
		{
		case '0': ans += 6; break;
		case '1': ans += 2; break;
		case '2': ans += 5; break;
		case '3': ans += 5; break;
		case '4': ans += 4; break;
		case '5': ans += 5; break;
		case '6': ans += 6; break;
		case '7': ans += 3; break;
		case '8': ans += 7; break;
		case '9': ans += 6; break;
		}
	}
	cout << ans << endl;
}
