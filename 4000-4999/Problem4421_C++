#include <iostream>
#include <algorithm>
#include <vector>
#include <map>

using namespace std;


int main()
{
	int l, r, x, N, Q;
	map<int, vector<int>> pos;
	scanf("%d", &N);
	for (int i = 0; i < N; ++i)
	{
		scanf("%d", &x);
		pos[x].push_back(i);
	}
	scanf("%d", &Q);
	while (Q--)
	{
		scanf("%d %d %d", &l, &r, &x);
		l--; r--;
		printf("%d", (int)(pos.count(x) && lower_bound(pos[x].begin(), pos[x].end(), l) != upper_bound(pos[x].begin(), pos[x].end(), r)));
	}
	return 0;
}
