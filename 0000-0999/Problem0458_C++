#include <iostream>
#include <fstream>

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

bool a[100][100], b[100][100];
char ch, c[4];
int n, m;

int main()
{
	cin >> m >> n;
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
		{
			cin >> ch;
			if (ch == '1') a[i][j] = true;
		}
	for (int i = 0; i < n; i++)
		for (int j = 0; j < m; j++)
		{
			cin >> ch;
			if (ch == '1') b[i][j] = true;
		}
	cin >> c[0] >> c[1] >> c[2] >> c[3];
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
			if (!a[i][j] && !b[i][j]) cout << c[0];
			else if (!a[i][j] && b[i][j]) cout << c[1];
			else if (a[i][j] && !b[i][j]) cout << c[2];
			else cout << c[3];
			cout << endl;
	}
}
