#include <iostream>
#include <fstream>

using namespace std;

fstream in("input.txt", ios::in), out("output.txt", ios::out);

typedef long long ll;

ll n, m;

ll gcd(ll a, ll b)
{
	while (b)
	{
		a %= b;
		swap(a, b);
	}
	return a;
}

int main()
{
	cin >> n >> m;
	if (n == 1 || m == 1) cout << 1 << endl;
	else cout << gcd(n, m) << endl;
}
